{
  "EntityFrameworkConfiguration": {
    "ConnectionString": ""
  },
  "AzureServiceBus": {
    "ConnectionString": ""
  },
  "TokenConfiguration": {
    "Secret": "GSP.WebApi_Token",
    "ExpiresInDay": 1
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Resources": {
    "EventBus": "AzureServiceBus"
  },
  "AllowedHosts": "*",
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "$domainName$ API health check result",
        "Uri": "/health-check-api"
      }
    ],
    "Webhooks": [],
    "EvaluationTimeinSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}